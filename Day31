package pkg100daysofcoding;

public class Day31_LogikaAnd {
    public static void main(String[] args) {
        int nilaiUjian = 80;
        int nilaiPraktik = 75;

        // Menggunakan operator logika AND untuk menentukan kelulusan
        boolean lulus = (nilaiUjian >= 70) && (nilaiPraktik >= 70);

        System.out.println("Status Lulus: " + lulus);
    }
}

    

